<h1>Editing event</h1>

<% form_for [:admin, @event] do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :date %><br />
    <%= f.date_select :date %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', [:admin, @event] %> |
<%= link_to 'Back', admin_events_path %>
